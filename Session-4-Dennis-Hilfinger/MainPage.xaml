<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Session_4_Dennis_Hilfinger.MainPage"
             Title="Import Survey Data">
    <VerticalStackLayout>

        <HorizontalStackLayout 
            HorizontalOptions="Center"
            VerticalOptions="Center"
            Spacing="50"
            Margin="0, 50, 0, 0">
            <Button 
                Clicked="ResultsSummary"
                Text="View Results Summary"
                WidthRequest="200"
                HeightRequest="50"/>
            <Button 
                Clicked="ImportData"
                Text="Import Data"
                WidthRequest="150"
                HeightRequest="50"/>
        </HorizontalStackLayout>
    </VerticalStackLayout>

</ContentPage>
